{
  "extends": "../.eslintrc",
  "plugins": [
    "markdown"
  ],
  "env": {
    "amd": 1,
    "browser": 1,
    "es6": 0,
    "node": 0
  },
  "rules": {
    "no-var": 0,
    "no-const-assign": 0,
    "prefer-const": 0,
    "eol-last": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "no-console": 0,
    "no-use-before-define": 0
  },
  "globals": {
    "ally": true
  }
}